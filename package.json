{
  "name": "warp-keeper",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:llllllluc/warp-keeper.git",
  "author": "llllllluc <58892938+llllllluc@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@skip-mev/skipjs": "^1.4.0",
    "@terra-money/terra.js": "^3.1.7",
    "@terra-money/warp-sdk": "^0.1.20",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "redis": "^4.6.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "postinstall": "husky install",
    "clean": "tsc --build --clean",
    "build": "yarn clean && tsc",
    "start-redis": "yarn reset-redis && redis-server",
    "start-redis-cli": "redis-cli",
    "reset-redis": "rm dump.rdb",
    "start-bot": "ts-node --esm --experimentalSpecifierResolution node src/bot.ts",
    "create-account": "ts-node --esm --experimentalSpecifierResolution node src/scripts/tester/create_account.ts",
    "deposit-account": "ts-node --esm --experimentalSpecifierResolution node src/scripts/tester/deposit_account.ts",
    "withdraw-account": "ts-node --esm --experimentalSpecifierResolution node src/scripts/tester/withdraw_account.ts",
    "create-job": "ts-node --esm --experimentalSpecifierResolution node src/scripts/tester/create_job.ts",
    "update-job": "ts-node --esm --experimentalSpecifierResolution node src/scripts/tester/update_job.ts",
    "execute-job": "ts-node --esm --experimentalSpecifierResolution node src/scripts/keeper/execute_job.ts",
    "evict-job": "ts-node --esm --experimentalSpecifierResolution node src/scripts/keeper.evict_job.ts"
  },
  "type": "module"
}

{
  "name": "warp-keeper",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:llllllluc/warp-keeper.git",
  "author": "llllllluc <58892938+llllllluc@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@sentry/node": "^7.44.2",
    "@sentry/tracing": "^7.44.2",
    "@skip-mev/skipjs": "^1.4.0",
    "@terra-money/terra.js": "^3.1.7",
    "@terra-money/warp-sdk": "^0.1.21",
    "redis": "^4.6.4"
  },
  "lint-staged": {
    "./src/{**/*,*}.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "format": "prettier --write .",
    "format-check": "prettier --ignore-path .gitignore --check .",
    "lint": "yarn format-check && eslint . && yarn build",
    "clean": "yarn tsc --build --clean && rm -rf build",
    "build": "yarn clean && tsc",
    "start-redis": "yarn reset-redis && redis-server",
    "start-redis-cli": "redis-cli",
    "reset-redis": "rm dump.rdb",
    "start-collector-dev": "ts-node src/bots/collector.ts",
    "start-monitor-dev": "ts-node src/bots/monitor.ts",
    "start-executor-dev": "ts-node src/bots/executor.ts",
    "start-collector-prod": "node build/bots/executor.js",
    "start-monitor-prod": "node build/bots/monitor.js",
    "start-executor-prod": "node build/bots/executor.js",
    "start-warp-keeper-in-docker": "docker-compose up --build",
    "create-account": "ts-node src/scripts/tester/create_account.ts",
    "deposit-account": "ts-node src/scripts/tester/deposit_account.ts",
    "withdraw-account": "ts-node src/scripts/tester/withdraw_account.ts",
    "create-job": "ts-node src/scripts/tester/create_job.ts",
    "update-job": "ts-node src/scripts/tester/update_job.ts",
    "execute-job": "ts-node src/scripts/keeper/execute_job.ts",
    "evict-job": "ts-node src/scripts/keeper/evict_job.ts"
  }
}
